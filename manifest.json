{
  "name": "Extensiel",
  "description": "Just makes content bettel",
  "version": "1.1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Glory to Albania"
  },
  "icons": {
    "32": "assets/icons/icon_32.png",
    "48": "assets/icons/icon_48.png",
    "128": "assets/icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/assets/albania_flag.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}